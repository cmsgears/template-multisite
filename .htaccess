BrowserMatch MSIE ie
Header set X-UA-Compatible "IE=Edge,chrome=1" env=ie

RewriteEngine On

# pass all the non uploads and non admin requests to frontend application
RewriteCond %{REQUEST_URI} !^/(cmgdemoblog/uploads)
RewriteCond %{REQUEST_URI} !^/(cmgdemoblog/backend/web|cmgdemoblog/backend)
RewriteRule !(frontend/web) /cmgdemoblog/frontend/web%{REQUEST_URI} [L]

# pass all the admin requests to admin application
RewriteCond %{REQUEST_URI} ^/(cmgdemoblog/backend)
RewriteRule !(backend/web) /cmgdemoblog/backend/web%{REQUEST_URI} [L]

# handle frontend application requests
RewriteCond %{REQUEST_URI} ^/cmgdemoblog/frontend/web
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)\?*$ /cmgdemoblog/frontend/web/index.php/$1 [L,QSA]

# handle admin application requests
RewriteCond %{REQUEST_URI} ^/cmgdemoblog/backend/web
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)\?*$ /cmgdemoblog/backend/web/index.php/$1 [L,QSA]
